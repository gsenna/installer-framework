<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>CsoundInstaller</Name>
    <Version>1.0.0</Version>
    <Title>Csound Installer</Title>
    <Publisher>Csound</Publisher>
    <StartMenuDir>Csound</StartMenuDir>
    <TargetDir>@ApplicationsDir@/Csound6</TargetDir>
    <RemoteRepositories>
        <Repository>
             <Url>https://raw.githubusercontent.com/gsenna/installer-framework/master/installer-repo/linux</Url>
             <Enabled>1</Enabled>
             <DisplayName>Github Csound Installer Repository</DisplayName>
        </Repository>
    </RemoteRepositories>
</Installer>
